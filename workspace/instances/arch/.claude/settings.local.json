{
  "permissions": {
    "allow": [
      "WebSearch"
    ]
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node D:/projects/hivemind/workspace/scripts/arch-hooks.js",
            "timeout": 10
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node D:/projects/hivemind/workspace/scripts/arch-hooks.js",
            "timeout": 5
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "node D:/projects/hivemind/workspace/scripts/arch-hooks.js",
            "timeout": 5
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node D:/projects/hivemind/workspace/scripts/arch-hooks.js",
            "timeout": 5
          }
        ]
      }
    ]
  }
}
